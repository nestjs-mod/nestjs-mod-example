{
  "name": "@project-name/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "_____prod infra_____": "_____prod infra_____",
    "start": "npm run nx:many -- -t=start",
    "build": "npm run generate && npm run tsc:lint && npm run nx:many -- -t=build --skip-nx-cache=true",
    "start:prod:app-name": "npm run nx -- start app-name --host=0.0.0.0",
    "build:prod:app-name": "npm run nx -- build app-name",
    "_____docs_____": "_____docs_____",
    "docs:infrastructure": "export NODE_ENV=infrastructure && npm run nx:many -- -t=start --parallel=1",
    "_____dev infra_____": "_____dev infra_____",
    "serve:dev": "npm run nx:many -- -t=serve --host=0.0.0.0",
    "serve:dev:app-name": "npm run nx -- serve app-name --host=0.0.0.0",
    "_____lint_____": "_____lint_____",
    "lint": "npm run tsc:lint && npm run nx:many -- -t=lint",
    "lint:fix": "npm run tsc:lint && npm run nx:many -- -t=lint --fix",
    "tsc:lint": "tsc --noEmit -p tsconfig.base.json",
    "_____tests_____": "_____tests_____",
    "test": "npm run nx:many -- -t=test --skip-nx-cache=true --passWithNoTests --output-style=stream-without-prefixes",
    "_____utils_____": "_____utils_____",
    "generate": "npm run nx:many -- -t=generate --skip-nx-cache=true && npm run make-ts-list && npm run lint:fix",
    "nx": "nx",
    "dep-graph": "npm run nx -- dep-graph",
    "nx:many": "npm run nx -- run-many --exclude=@project-name/source --all",
    "make-ts-list": "./node_modules/.bin/rucken make-ts-list",
    "manual:prepare": "npm run generate && npm run build && npm run docs:infrastructure && npm run test",
    "_____pm2 prod infra_____": "_____pm2 prod infra_____",
    "pm2:start": "./node_modules/.bin/pm2 start ./ecosystem.config.json",
    "pm2:stop": "./node_modules/.bin/pm2 delete all"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@nestjs-mod/schematics": "^2.1.8",
    "@nestjs-mod/testing": "2.5.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/jest": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/node": "17.2.8",
    "@nx/webpack": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@theunderscorer/nx-semantic-release": "^2.10.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "fast-glob": "^3.3.2",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "lint-staged": "^15.2.0",
    "nx": "17.2.8",
    "prettier": "^2.6.2",
    "react-refresh": "^0.10.0",
    "rucken": "^4.6.4",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.2.2",
    "url-loader": "^4.1.1",
    "verdaccio": "^5.0.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "dependencies": {
    "@nestjs-mod/common": "2.5.3",
    "@nestjs-mod/pino": "1.6.2",
    "@nestjs-mod/pm2": "1.4.2",
    "@nestjs-mod/reports": "2.5.3",
    "@nestjs-mod/terminus": "1.5.2",
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/terminus": "^10.2.0",
    "axios": "^1.0.0",
    "case-anything": "^2.1.13",
    "class-validator": "^0.14.0",
    "dotenv": ">=16.3.1",
    "nestjs-pino": "^4.0.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  }
}